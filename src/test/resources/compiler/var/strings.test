func p_str(s string, i int) string {
	if(i) {
		print("Hello, ");
	} else {
		print("hello, ");
	}
	print(s);
	return s;
}

func p_str(s0 string, s1 string) string {
   p_str(s0, 1);
   print(" and ");
   p_str(s1, 0);
   return "!";
}

func main() int {
    var s string;
    s = p_str("世界", "everyone else");
    print(s);
    return 0;
}

